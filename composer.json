{
  "name": "dcarbone/soap-plus",
  "type": "library",
  "description": "PHP SoapClient wrapper class",
  "keywords": [
    "php",
    "soap",
    "soapclient"
  ],
  "homepage": "https://github.com/dcarbone/soap-plus",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Daniel Carbone",
      "email": "daniel.p.carbone@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "ext-curl": "*",
    "ext-libxml": "*",
    "ext-simplexml": "*",
    "ext-soap": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "DCarbone\\SoapPlus\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DCarbone\\SoapPlus\\Tests\\": "tests/"
    }
  }
}
